{
    "compilerOptions": {
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,

        "target": "ES2016",
        "module": "CommonJS",
        "outDir": ".build",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*", "src/app.ts", "Members", "Members", ".vscode/config.ts"]
}
